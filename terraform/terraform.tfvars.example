# vpc_az_count = 2
# vpc_single_nat_gateway = true  # set false for one NAT per AZ

# create_sagemaker_domain = true
# sagemaker_domain_name = "aws-mls-prep-domain-dev"
# sagemaker_user_profile_name = "aws-mls-prep"

# create_prediction_user = true
# prediction_username = "ml_user_predict"

